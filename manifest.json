{
  "manifest_version": 2,
  "name": "SIWO",
  "description": "Share Info With Other",
  "version": "0.0.1",
  "icons": {
    "128": "icon.png",
    "48": "icon48.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs"],
  "content_scripts": [{
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery.min.js", "main.js"],
      "run_at": "document_start"
  }]
}
